{
    "version": 2,
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "client/dist",
    "installCommand": "npm install",
    "functions": {
      "api/analyze.py": {
        "runtime": "python3.9"
      },
      "api/data.py": {
        "runtime": "python3.9"
      },
      "api/health.py": {
        "runtime": "python3.9"
      },
      "api/session.py": {
        "runtime": "python3.9"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }